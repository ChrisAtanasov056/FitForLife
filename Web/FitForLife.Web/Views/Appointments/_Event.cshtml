@model FitForLife.Web.ViewModels.Events.EventViewModel

    <script src="//code.jquery.com/jquery-1.11.1.min.js"></script>
    <link href="//netdna.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap.min.css" rel="stylesheet" id="bootstrap-css">
    <script src="//netdna.bootstrapcdn.com/bootstrap/3.2.0/js/bootstrap.min.js"></script>
    <link href="~/css/Calendar.css" rel="stylesheet" />
    <script src="~/js/Calendar.js"></script>
    


<div class="events">
        <div class="event">
            <h4>@Model.Class.Name</h4>
            <p>@Model.StartEvent.ToString("t") - @Model.EndEvent.ToString("t")</p>
            <div class="btn btn-info btn-sm">
                <a asp-controller="Appointments" asp-action="Details" asp-route-id="@Model.Id">Details</a>
            </div>
        </div>
    </div>


